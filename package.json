{
  "name": "lazyapp",
  "version": "0.0.0",
  "scripts": {
    "build-lazy": "ng build lazyC && ng build lazyB && ng build lazyA",
    "copy-lazy": "copyfiles -u 3 dist/lazy-a/bundles/lazy-a.umd.js dist/lazy-b/bundles/lazy-b.umd.js dist/lazy-c/bundles/lazy-c.umd.js dist/lazyapp/deps",
    "copy-deps": "copyfiles -u 3 node_modules/ngx-logger/bundles/ngx-logger.umd.js node_modules/vlq/dist/vlq.js dist/lazyapp/deps",
    "start-server": "http-server ./dist/lazyapp",
    "ng": "ng",
    "start": "npm run build-lazy && npm run build && npm run copy-lazy && npm run copy-deps && npm run start-server",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.9",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "~10.0.9",
    "@angular/compiler": "~10.0.9",
    "@angular/core": "~10.0.9",
    "@angular/forms": "~10.0.9",
    "@angular/material": "^10.1.3",
    "@angular/platform-browser": "~10.0.9",
    "@angular/platform-browser-dynamic": "~10.0.9",
    "@angular/router": "~10.0.9",
    "ngx-logger": "^4.1.9",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.6",
    "@angular-devkit/build-ng-packagr": "~0.1000.6",
    "@angular/cli": "~10.0.6",
    "@angular/compiler-cli": "~10.0.9",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.3.0",
    "http-server": "^0.12.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  }
}
